{
    "scriptFile": "../news_ingestion.py",
    "entryPoint": "main",
    "bindings": [
        {
            "name": "timer",
            "type": "timerTrigger",
            "direction": "in",
            "schedule": "0 * * * * *",
            "runOnStartup": false,
            "useMonitor": true
        },
        {
            "name": "outputEvent",
            "type": "eventHub",
            "direction": "out",
            "eventHubName": "news",
            "connection": "AZURE_EVENTHUB_CONNECTION_STRING"
        }
    ]
}